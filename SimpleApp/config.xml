<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="uk.co.linkinfotec.SimpleApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>SimpleApp</name>
  <description>A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">Apache Cordova Team </author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.1" />
  <vs:plugin name="org.apache.cordova.splashscreen" version="0.3.3" />
  <vs:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
  <vs:plugin name="com.phonegap.plugins.PushPlugin" version="2.4.0" />

  <vs:platformSpecificValues />
  <gap:plugin name="org.apache.cordova.pushplugin" />
  <gap:plugin name="org.apache.cordova.inappbrowser" />
  <gap:plugin name="org.apache.cordova.splashscreen" />
  
</widget>
